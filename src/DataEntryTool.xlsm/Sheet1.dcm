VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "Sheet1"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Option Explicit

'====================================================================================================
' ワークシート変更イベント
'----------------------------------------------------------------------------------------------------
' IN : target 対象のセル範囲
'====================================================================================================
Private Sub Worksheet_Change(ByVal target As Range)
    Dim View As DataEntryView
    Set View = New DataEntryView
    Call View.ChangeCellRange(ThisWorkbook.Worksheets(1), target)
    Set View = Nothing
End Sub

