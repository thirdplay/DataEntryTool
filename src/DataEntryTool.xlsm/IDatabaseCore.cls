VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "IDatabaseCore"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Option Explicit

'====================================================================================================
'
' 各データベースの中を取り持つインターフェース
'
'====================================================================================================

'====================================================================================================
' データベースの接続文字列を取得します
'----------------------------------------------------------------------------------------------------
' IN : xSetting 設定
' OUT: 接続文字列
'====================================================================================================
Public Function GetConnectStr(xSetting As Setting) As String
End Function


'====================================================================================================
' カラム定義取得クエリを取得します
'----------------------------------------------------------------------------------------------------
' OUT: カラム定義取得クエリ
'====================================================================================================
Public Function GetColumnDefinitionQuery() As String
End Function


'====================================================================================================
' データ種別が文字列かどうか判定する
'----------------------------------------------------------------------------------------------------
' IN : dataType データ種別
'====================================================================================================
Public Function IsDataTypeString(dataType As String) As Boolean
End Function


